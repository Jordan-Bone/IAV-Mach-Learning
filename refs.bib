	journal = {Virology},
	keywords = {Canine influenza, Influenza A, Chemokine, Pathogenesis, Hyperchemokinemia, H3N2},
	number = {1},
	pages = {57-63},
	title = {Severe canine influenza in dogs correlates with hyperchemokinemia and high viral load},
	url = {https://www.sciencedirect.com/science/article/pii/S0042682211002182},
