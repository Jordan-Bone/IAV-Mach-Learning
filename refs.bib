	doi = {10.3390/pathogens5040064},
	journal = {Pathogens},
	journal-full = {Pathogens (Basel, Switzerland)},
	keywords = {canarypox; equine influenza; horse; update; vaccination},
	month = {Nov},
	number = {4},
	pmc = {PMC5198164},
	pmid = {27897990},
	pst = {epublish},
	title = {How to Meet the Last OIE Expert Surveillance Panel Recommendations on Equine Influenza (EI) Vaccine Composition: A Review of the Process Required for the Recombinant Canarypox-Based EI Vaccine},
	volume = {5},
