	doi = {10.1016/j.vetmic.2013.11.039},
	journal = {Vet Microbiol},
	journal-full = {Veterinary microbiology},
	keywords = {Antigenic cartography; Equine influenza virus; H3N8; Neuraminidase; Surveillance},
	mesh = {Amino Acid Sequence; Animals; Europe; Hemagglutinins, Viral; Horse Diseases; Horses; Influenza A Virus, H3N8 Subtype; Models, Molecular; Molecular Sequence Data; Neuraminidase; Orthomyxoviridae Infections; Phylogeny; Population Surveillance; Protein Structure, Tertiary; Sequence Alignment; United Arab Emirates; United States},
	month = {Mar},
