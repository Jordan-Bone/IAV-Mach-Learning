	mesh = {Animals; Computer Simulation; DNA Primers; Genome, Viral; Horse Diseases; Horses; Humans; Influenza A Virus, H3N8 Subtype; Influenza A virus; Influenza, Human; Multiplex Polymerase Chain Reaction; Orthomyxoviridae Infections; Phylogeny},
	month = {Feb},
