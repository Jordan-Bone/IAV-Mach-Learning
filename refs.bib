	journal-full = {Virology journal},
	keywords = {Distribution; Diversity; Influenza virus; Nomenclature; Phylogenetics; Protein},
	mesh = {Amino Acid Sequence; Animals; Birds; Chiroptera; Dogs; Evolution, Molecular; Genes, Viral; Genetic Variation; Horses; Humans; Influenza A virus; Phylogeny; Swine},
