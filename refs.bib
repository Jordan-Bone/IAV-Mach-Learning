	keywords = {Brazil; Epidemic; Equine influenza; Florida clade 1; Haemagglutinin; Neuraminidase},
	mesh = {Amino Acid Sequence; Animals; Disease Outbreaks; Geography, Medical; Hemagglutinin Glycoproteins, Influenza Virus; History, 21st Century; Horse Diseases; Horses; Influenza A Virus, H3N8 Subtype; Influenza Vaccines; Models, Molecular; Molecular Sequence Data; Neuraminidase; Orthomyxoviridae Infections; Phylogeny; Protein Conformation; RNA, Viral; Sequence Alignment; South America; Vaccination; Viral Proteins},
	month = {Mar},
