	journal = {Journal of General Virology},
	keywords = {Mpox},
	number = {6},
	publisher = {Microbiology Society},
	title = {Smallpox vaccination campaigns resulted in age-associated population cross-immunity against monkeypox virus},
	type = {Journal Article},
	url = {https://www.microbiologyresearch.org/content/journal/jgv/10.1099/jgv.0.001999},
	volume = {105},
	year = {2024},
	bdsk-url-1 = {https://www.microbiologyresearch.org/content/journal/jgv/10.1099/jgv.0.001999},
