	keywords = {Equine; Mongolia; infectious disease outbreaks; influenza virus; sentinel surveillance},
	mesh = {Animals; Female; Horse Diseases; Horses; Influenza A Virus, H3N8 Subtype; Male; Molecular Sequence Data; Mongolia; Orthomyxoviridae Infections; Phylogeny},
	month = {Sep},
	number = {5},
	pages = {659-65},
	pmc = {PMC3626732},
