	journal = {Nature Reviews Microbiology},
	number = {5},
	pages = {321--328},
	title = {Prisoners of war ---host adaptation and its constraints on virus evolution},
	url = {https://doi.org/10.1038/s41579-018-0120-2},
	volume = {17},
	year = {2019},
