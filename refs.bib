	issn = {0022-2836},
	journal = {Journal of Molecular Biology},
	number = {3},
	pages = {357-386},
	title = {Conformation of amino acid side-chains in proteins},
	url = {https://www.sciencedirect.com/science/article/pii/0022283678904084},
	volume = {125},
	year = {1978},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/0022283678904084},
