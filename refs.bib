	doi = {10.1111/evj.12290},
	journal = {Equine Vet J},
	journal-full = {Equine veterinary journal},
	keywords = {cross-neutralisation; equine antisera; equine influenza; horse; vaccine update},
	mesh = {Amino Acid Substitution; Animals; Hemagglutinins, Viral; Horse Diseases; Horses; Influenza Vaccines; Molecular Sequence Annotation; Molecular Sequence Data; Neutralization Tests; Orthomyxoviridae Infections; Phylogeny},
	month = {Jul},
	number = {4},
	pages = {456-62},
	pmid = {24773030},
	pst = {ppublish},
	title = {The potential impact of a single amino-acid substitution on the efficacy of equine influenza vaccines},
	volume = {47},
	year = {2015},
