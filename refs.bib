	journal-full = {Influenza and other respiratory viruses},
	keywords = {Argentina; Florida clade 1; equine influenza},
	mesh = {Animals; Argentina; Disease Outbreaks; Horse Diseases; Horses; Influenza A Virus, H3N8 Subtype; Nasopharynx; Orthomyxoviridae Infections; Phylogeny; Real-Time Polymerase Chain Reaction; Reverse Transcriptase Polymerase Chain Reaction; Sequence Analysis, RNA; Uruguay},
	month = {Jan},
	number = {1},
	pages = {37-46},
