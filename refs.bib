	journal = {Virus Res},
	journal-full = {Virus research},
	mesh = {Animals; Base Sequence; Conserved Sequence; DNA, Viral; Evolution, Molecular; Genes, Viral; Humans; Influenza A virus; Molecular Sequence Data; Mutagenesis, Insertional; Phylogeny; Species Specificity; Swine; Viral Nonstructural Proteins},
	month = {Jun},
