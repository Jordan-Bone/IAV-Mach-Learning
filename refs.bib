	mesh = {Amino Acid Sequence; Animals; Base Sequence; Cloning, Molecular; Ducks; Escherichia coli; Genes, Viral; Horses; Influenza A virus; Models, Molecular; Molecular Sequence Data; Mutation; Neuraminidase; Phylogeny; Protein Structure, Secondary; RNA, Viral; Sequence Homology, Amino Acid; Sequence Homology, Nucleic Acid; Viral Structural Proteins},
	month = {Apr},
