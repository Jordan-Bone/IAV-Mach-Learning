	month = {Sep},
	number = {29-30},
	pages = {4101-9},
	pmid = {15364463},
	pst = {ppublish},
	title = {Evidence supporting the inclusion of strains from each of the two co-circulating lineages of H3N8 equine influenza virus in vaccines},
