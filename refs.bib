	mesh = {Adaptation, Physiological; Animals; Biological Evolution; Codon; Gene Expression Regulation, Viral; Genetic Code; Genome, Viral; Horse Diseases; Horses; Host-Pathogen Interactions; Influenza A Virus, H3N8 Subtype; Influenza A Virus, H7N7 Subtype; Models, Statistical; Mutation Rate; Orthomyxoviridae Infections; RNA, Transfer; Species Specificity; Virus Replication},
	number = {4},
	pages = {e0154376},
	pmc = {PMC4847779},
	pmid = {27119730},
	pst = {epublish},
	title = {Revelation of Influencing Factors in Overall Codon Usage Bias of Equine Influenza Viruses},
