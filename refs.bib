	doi = {10.1093/infdis/jiv563},
	eprint = {https://academic.oup.com/jid/article-pdf/213/8/1229/7425163/jiv563.pdf},
	issn = {0022-1899},
	journal = {The Journal of Infectious Diseases},
	month = {11},
	number = {8},
	pages = {1229-1236},
	title = {{Substantial Influenza Vaccine Effectiveness in Households With Children During the 2013--2014 Influenza Season, When 2009 Pandemic Influenza A(H1N1) Virus Predominated}},
