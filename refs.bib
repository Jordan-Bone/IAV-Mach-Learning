	keywords = {Food safety, , Alternative poultry production, Boosted trees, Random forest, Gradient boosting machine, Predictive microbiology},
	pages = {47-55},
	title = {Comparison between random forest and gradient boosting machine methods for predicting Listeria spp. prevalence in the environment of pastured poultry farms},
	url = {https://www.sciencedirect.com/science/article/pii/S0963996919302133},
	volume = {122},
