	keywords = {epidemio-surveillance; equine influenza; horse; outbreak; phylogenetic analysis; surveillance network},
	mesh = {Animals; Cross-Sectional Studies; Epidemiological Monitoring; France; Horse Diseases; Horses; Influenza A virus; Orthomyxoviridae Infections; Phylogeny; Population Surveillance; Real-Time Polymerase Chain Reaction; Reproducibility of Results; Retrospective Studies; Reverse Transcriptase Polymerase Chain Reaction; Sensitivity and Specificity},
