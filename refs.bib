	keywords = {Asia; H3N8; H7N7; equine influenza; evolution; influenza; signature pattern; vaccine},
	mesh = {Animals; Asia; Disease Outbreaks; Evolution, Molecular; Horse Diseases; Horses; Influenza A Virus, H3N8 Subtype; Influenza A Virus, H7N7 Subtype; Orthomyxoviridae Infections; Phylogeny; Phylogeography; Viral Proteins},
