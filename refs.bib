	doi = {10.1016/j.vetmic.2015.10.028},
	journal = {Vet Microbiol},
	journal-full = {Veterinary microbiology},
	keywords = {EIV; Equine influenza virus; NEP; NS1; Phylogenetic analysis},
	mesh = {Animals; Gene Expression Regulation, Viral; Horse Diseases; Horses; Influenza A virus; Orthomyxoviridae Infections; Phylogeny; Poland; Viral Nonstructural Proteins},
	pages = {95-101},
	pmid = {26711034},
