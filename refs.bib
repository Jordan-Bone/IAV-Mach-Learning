	keywords = {Antigenic drift; Equine influenza; Horse; Pathogenicity markers; Surveillance},
	mesh = {Amino Acid Sequence; Amino Acid Substitution; Animals; Binding Sites; Evolution, Molecular; France; Genome; Hemagglutinin Glycoproteins, Influenza Virus; Horse Diseases; Horses; Influenza A Virus, H3N8 Subtype; Neuraminidase; Orthomyxoviridae Infections; Sequence Alignment},
	month = {05},
	pages = {210-217},
	pmid = {28292661},
